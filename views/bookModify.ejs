        <div id="page-wrapper">
            <div id="page-inner">
                <div class="row">
                    <div class="col-md-12">
                    <center><h2>도서정보수정</h2></center>
                    </div>
                </div>
                <br/><br/>
				<div class="container">
				  <form class="form-horizontal" role="form" action="/bookModify" method='post'>
				    <fieldset>
              <div class="form-group">
				        <label class="col-sm-3 control-label" for="booksNumber">도서번호</label>
				        <div class="col-sm-7">
				          <input type="text" class="form-control" value="<%=books[0].booksNumber%>" readonly="readonly" name="booksNumber" id="booksNumber" />
				        </div>
				      </div>
			        <div class="form-group">
				        <label class="col-sm-3 control-label" for="booksName">도서명</label>
				        <div class="col-sm-7">
				          <input type="text" class="form-control" value="<%=books[0].booksName%>" name="booksName" id="booksName" placeholder="도서명을 입력하세요">
				        </div>
				      </div>
				      <div class="form-group">
				        <label class="col-sm-3 control-label" for="booksMade">출판사</label>
				        <div class="col-sm-7">
				          <input type="text" class="form-control" value="<%=books[0].booksMade%>" name="booksMade" id="booksMade" placeholder="출판사를 입력하세요">
				        </div>
				      </div>
				      <div class="form-group">
				        <label class="col-sm-3 control-label" for="bookskind">도서분류</label>
				        <div class="col-sm-7">
				          <div class="row">
				            <div class="col-xs-3">
				              <select class="form-control col-sm-2" name="bookskind" id="bookskind">
				                <option>--분류--</option>
                        <% for(var k in bookskind){%>
				                <option value="<%=bookskind[k].bookskindsNumber%>"><%=bookskind[k].bookskindsname%></option>
                        <%}%>
				              </select>
				            </div>
				           </div>
				          </div>
				        </div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="booksAuthor">저자</label>
							<div class="col-sm-7">
							  <input type="text" class="form-control" value="<%=books[0].booksAuthor%>" name="booksAuthor" id="booksAuthor" placeholder="출판사를 입력하세요"/>
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="firstRentDay">최초대여일</label>
							<div class="col-sm-7">
							  <input type="date" class="form-control" value="<%=books[0].firstRentDay%>" name="firstRentDay" id="firstRentDay"/>
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="booksLendingPossible">대여가능여부</label>
							<div class="col-sm-7">
							  <label class="radio-inline">
							  	<input type="radio"  name="booksLendingPossible" value="1"/>대여가능
							  </label>
							  <label class="radio-inline">
							  	<input type="radio"  name="booksLendingPossible" value="0"/>대여불가능
							  </label>
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="booksLendingDay">대여일수</label>
							<div class="col-sm-7">
							  <input type="text" class="form-control" value="<%=books[0].booksLendingPossible%>" name="booksLendingDay" id="booksLendingDay" placeholder="총대여일수를 입력하세요"/>
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="booksLendingCount">대여횟수</label>
							<div class="col-sm-7">
							  <input type="text" class="form-control" value="<%=books[0].booksLendingCount%>" name="booksLendingCount" id="booksLendingCount" placeholder="총대여횟수를 입력하세요"/>
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="booksDamage">파손여부</label>
							<div class="col-sm-7">
							  <label class="radio-inline">
							  	<input type="radio"  name="booksDamage" value="1"/>파손됨
							  </label>
							  <label class="radio-inline">
							  	<input type="radio"  name="booksDamage" value="0"/>멀쩡함
							  </label>
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="booksDmageDate">파손날짜</label>
							<div class="col-sm-7">
							  <input type="date" class="form-control" name="booksDmageDate" id="booksDmageDate"/>
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label" for="bookskind">도서분류</label>
					        <div class="col-sm-9">
					          <div class="row">
					        	<div class="col-xs-3">
					              <select class="form-control" name="libraryNumber">
                          <option>--도서관선택--</option>
                          <% for(var l in librResult) {%>
					              	<option value="<%=librResult[l].libraryNumber%>"><%=librResult[l].libraryName%></option>
                          <%}%>
                        </select>
					            </div>
					          </div>
					        </div>
				      </div>
              <div class="form-group">
				        <div class="col-sm-offset-3 col-sm-9">
				          <input type="submit" class="btn btn-success" value="수정"/>
				        </div>
				      </div>
				    </fieldset>
				  </form>
				</div>
                 <!-- /. ROW  -->
